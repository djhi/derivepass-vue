{
  "name": "derivepass-electron",
  "version": "1.0.0",
  "description": "Electron app for DerivePass",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.electron.derivepass",
    "productName": "DerivePass",
    "files": [
      "main.js",
      "preload.js",
      { "from": "../dist", "to": "static" }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/derivepass/derivepass-vue.git"
  },
  "keywords": [
    "derivepass",
    "electron"
  ],
  "author": "Fedor Indutny <fedor@indutny.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/derivepass/derivepass-vue/issues"
  },
  "homepage": "https://github.com/derivepass/derivepass-vue#readme",
  "devDependencies": {
    "electron": "^4.0.4",
    "electron-builder": "^20.38.5"
  },
  "dependencies": {}
}
